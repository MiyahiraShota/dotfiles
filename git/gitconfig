[alias]
	st = status
	co = checkout
	br = branch
	ci = commit
  sw = switch
  re = restore
	df = diff
	gr = grep
	graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
[push]
	default = matching
[color]
    diff = true
    ui = auto
[include]
    path = ~/.gitconfig.local
[core]
	autocrlf = false
	filemode = false
